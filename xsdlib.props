<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <xsd-path>$([System.IO.Path]::GetFullPath('$(MSBuildThisFileDirectory)'))</xsd-path>
  </PropertyGroup>
  <PropertyGroup>
    <ExecutablePath>$(xsd-path)bin;$(ExecutablePath)</ExecutablePath>
    <IncludePath>$(xsd-path)libxsd;$(IncludePath)</IncludePath>
  </PropertyGroup>
  <ItemGroup>
    <BuildMacro Include="xsd-path">
      <Value>$(xsd-path)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>